{
  "name": "angular-example",
  "version": "1.0.0",
  "description": "Angular 14 Example With Jest and Pure ESM",
  "author": "Ruben Balderas",
  "license": "MIT",
  "private": true,
  "type": "module",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "e2e": "ng e2e",
    "watch": "jest --watch",
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js",
    "test:watch": "jest --watch",
    "test:ci": "jest --runInBand"
  },
  "dependencies": {
    "@angular-devkit/build-angular": "^14.2.9",
    "@angular/animations": "^14.2.9",
    "@angular/common": "^14.2.9",
    "@angular/compiler": "^14.2.9",
    "@angular/compiler-cli": "14.2.9",
    "@angular/core": "^14.2.9",
    "@angular/forms": "^14.2.9",
    "@angular/localize": "^14.2.9",
    "@angular/platform-browser": "^14.2.9",
    "@angular/platform-browser-dynamic": "^14.2.9",
    "@angular/router": "^14.2.9",
    "@ng-bootstrap/ng-bootstrap": "^12.1.2",
    "@nrwl/jest": "15.0.13",
    "@popperjs/core": "^2.11.5",
    "@swc/jest": "0.2.23",
    "@types/jest": "29.2.2",
    "bootstrap": "^5.1.3",
    "bootstrap-icons": "^1.8.3",
    "d3-scale-chromatic": "3.0.0",
    "jest": "29.3.1",
    "jest-preset-angular": "12.2.2",
    "lodash": "4.17.21",
    "moment": "2.29.4",
    "rxjs": "^7.5.5",
    "ts-jest": "29.0.3",
    "ts-node": "10.9.1",
    "tslib": "^2.4.0",
    "typescript": "4.8.4",
    "zone.js": "^0.11.5"
  },
  "devDependencies": {
    "@angular/cli": "14.2.9",
    "@swc/core": "^1.3.14",
    "@types/node": "^16.11.36",
    "protractor": "~7.0.0"
  },
  "engines": {
    "node": ">=14.16"
  },
  "exports": {
    "./schematics/*": {
      "default": "./schematics/*.js"
    },
    "./package.json": {
      "default": "./package.json"
    },
    ".": {
      "types": "./core.d.ts",
      "esm2020": "./esm2020/core.mjs",
      "es2020": "./fesm2020/core.mjs",
      "es2015": "./fesm2015/core.mjs",
      "node": "./fesm2015/core.mjs",
      "default": "./fesm2020/core.mjs"
    },
    "./testing": {
      "types": "./testing/testing.d.ts",
      "esm2020": "./esm2020/testing/testing.mjs",
      "es2020": "./fesm2020/testing.mjs",
      "es2015": "./fesm2015/testing.mjs",
      "node": "./fesm2015/testing.mjs",
      "default": "./fesm2020/testing.mjs"
    }
  }
}
